      "source": [
        "# Numpy 가장 많이 사용되는 함수\n",
        "# 1. 원소 정렬\n",
        "import numpy as np\n",
        "# def -> 오름차순 정렬 형태\n",
        "array = np.array([15, 20, 5, 12, 7])\n",
        "np.save(\"./array.npy\", array)\n",
        "array_data = np.load('./array.npy')\n",
        "array_data.sort()\n",
        "print(\"오름차순 >> \", array_data)\n"
      ]
    },
    {
